{
  "name": "Security Analysis Team",
  "description": "Comprehensive network security scanning and analysis",
  "version": "2.0.0",
  "available_mcps": [
    "nmap_recon",
    "file_tools",
    "agent",
    "history"
  ],
  "agents": [
    {
      "agent_id": "network-scanner",
      "role": "Network Scanner",
      "responsibilities": [
        "Network reconnaissance and discovery",
        "Port scanning and service detection",
        "Vulnerability identification"
      ],
      "mcp_access": ["nmap_recon"]
    },
    {
      "agent_id": "security-analyst",
      "role": "Security Expert",
      "responsibilities": [
        "Security analysis and risk assessment",
        "Vulnerability analysis",
        "Security recommendations"
      ],
      "mcp_access": ["nmap_recon", "agent", "file_tools"]
    },
    {
      "agent_id": "documentation-specialist",
      "role": "Documentation Specialist",
      "responsibilities": [
        "Report generation",
        "Documentation of findings",
        "Executive summaries"
      ],
      "mcp_access": ["file_tools"]
    }
  ],
  "coordination": {
    "mode": "sequential",
    "share_context": true,
    "task_distribution": "automatic"
  },
  "tags": [
    "security",
    "network-analysis",
    "multi-agent"
  ],
  "author": "ADCL Platform"
}